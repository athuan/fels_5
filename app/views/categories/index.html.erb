<% provide(:title, 'All categories') %>
<h1>All categories</h1>

<%= will_paginate %>

<ul class="categories">
  <% @categories.each do |category| %>
    <li>
      <%= link_to category.name, category %>
      <%= category.content%>
    </li>
      <%= link_to "Start", "#",class: "btn btn-large btn-primary" %>

  <% end %>
</ul>

<%= will_paginate %>	